FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1e-10;

boundaryField
{

    frontAndBackPlanes 
    {
        type            empty;
    }

    parete
    {
        type            zeroGradient;
    }

    inflow
    {
        type            fixedValue;
        value           uniform 1e-10;
    }

    outflow
    {
        type            fixedValue;
        value           uniform 1e-10;
    }

}


